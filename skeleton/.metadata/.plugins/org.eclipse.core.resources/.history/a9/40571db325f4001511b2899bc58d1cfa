package generatedFile_v1;

public enum Direction {
	Right, Left, Top, Bottom;
	
	public Direction inverz() {
		switch (this)
		{
			case Right:
				return Direction.Left;
			case Left:
				return Direction.Right;
			case Top:
				return Direction.Bottom;
			case Bottom:
				return Direction.Top;
			default:
				return null;
		}
	}
}
