package generatedFile_v1;

import java.util.*;

/**
 * 
 */
public class ONeill {

    /**
     * 
     */
    private Direction dir;
    private Box box;
    private Floor floor;


    /**
     * 
     */
    public void move() {
    	LevelEntity entity = floor.getNeighbour(dir);
    	if(entity.moveAction(this)) {
    		floor.setONeill(null);
    	}
    	
    }

    /**
     * 
     */
    public void boxing() {
    	
    }

    /**
     * @param Box
     */
    public void setBox( Box b ) {
        box = b;
    }

    /**
     * 
     */
    public void kill() {
    	while(1 > 0)
    		System.out.println("Game Over");
    }

    /**
     * 
     */
    public void shoot() {
    	
    }

}